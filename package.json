{
  "name": "storybook-vite-react-component",
  "version": "0.0.0",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "start-storybook -p 3000",
    "build": "npm run build:vite && npm run build:storybook",
    "build:vite": "vite build",
    "build:storybook": "build-storybook --quiet --loglevel error --output-dir ./dist/storybook",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "styled-components": "*"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@types/babel__core": "^7.1.16",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.14",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^8.2.2",
    "camelcase": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "storybook-builder-vite": "^0.0.12",
    "styled-components": "^5.3.1",
    "typescript": "^4.4.2",
    "vite": "^2.5.3",
    "vite-plugin-dts": "^0.7.0"
  }
}
